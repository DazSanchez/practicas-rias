<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<FlowPane maxHeight="125.0" maxWidth="350.0" minHeight="125.0" minWidth="350.0" prefHeight="125.0" prefWidth="350.0" vgap="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox prefWidth="330.0">
            <children>
                <Label text="Registro exitoso" textFill="#00cf00">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox prefWidth="330.0">
            <children>
                <Label fx:id="messageLabel" text="Gracias por registrarte ${user}!">
                    <padding>
                        <Insets bottom="10.0" />
                    </padding>
                </Label>
            </children>
        </HBox>
        <HBox alignment="BOTTOM_RIGHT" prefHeight="26.0" prefWidth="330.0">
            <children>
                <Button fx:id="acceptBtn" defaultButton="true" mnemonicParsing="false" text="Aceptar" />
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</FlowPane>
